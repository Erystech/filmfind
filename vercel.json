{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/tmdb.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/tmdb",
      "dest": "api/tmdb.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}